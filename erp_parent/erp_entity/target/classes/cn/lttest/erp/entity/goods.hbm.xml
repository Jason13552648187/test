<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.lttest.erp.entity.Goods" table="goods" >
        <id name="uuid" >
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"> 
            	<param name="sequence_name">goods_seq</param> 
            </generator>
        </id>

        <property name="name" /> 
        <property name="origin" /> 
        <property name="producer" /> 
        <property name="unit" /> 
        <property name="inprice" /> 
        <property name="outprice" /> 
       <many-to-one name="goodstype" class="cn.lttest.erp.entity.Goodstype" lazy="false" column="goodstypeuuid">
       </many-to-one>
 
    </class>
</hibernate-mapping>
